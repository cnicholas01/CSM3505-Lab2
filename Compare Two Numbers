fun main(){
    var timeSpentToday = 300
    var timeSpentYesterday = 250
    println("Have I spent more time using my phone today: ${compareTime(timeSpentToday, timeSpentYesterday)}")
    
    timeSpentToday = 300
    timeSpentYesterday = 300
     println("Have I spent more time using my phone today: ${compareTime(timeSpentToday, timeSpentYesterday)}")
    
    timeSpentToday = 200
    timeSpentYesterday = 220
     println("Have I spent more time using my phone today: ${compareTime(timeSpentToday, timeSpentYesterday)}")
}

fun compareTime(timeToday: Int, timeYesterday: Int): Boolean{
    return timeToday > timeYesterday
}
